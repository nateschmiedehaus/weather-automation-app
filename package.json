{
  "name": "weather-intel-demo",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "clean:cache": "rm -rf node_modules/.vite dist tsconfig.tsbuildinfo || true",
    "clean-start": "npm run clean:cache && VITE_DEMO=1 vite",
    "demo": "npm run dev:sidecar",
    "open:sidecar": "bash ./bin/open-sidecar.sh",
    "predev:sidecar": "bash ./bin/preflight.sh",
    "dev:sidecar": "concurrently -k \"npm:dev\" \"npm:open:sidecar\"",
    "build": "tsc -b && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "lucide-react": "^0.460.0",
    "recharts": "^2.12.7",
    "framer-motion": "^11.11.1",
    "react-router-dom": "^6.26.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.3.1",
    "concurrently": "^9.1.0",
    "@types/node": "^22.7.4",
    "@types/react": "^18.3.7",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.6.2",
    "vite": "^5.4.8"
  }
}
