    if (calReady && spendReady && selectedSimDate) {
      runDemoPipeline();
    }
    // eslint-disable-next-line react-hooks/exhaustive-deps
  }, [platformMode, selectedBrand, selectedSimDate, simCalendar, adSpendHistory]);

