  // ==== PATCH E: DATE-AWARE GENERATORS ====
  const withDate = (iso?: string | null) => iso ? new Date(iso) : new Date();

  const generateSimPromosAt = useCallback((brandKey: string, isoDate: string | null, scen: ScenarioState): PromoSignal[] => {
